<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="calendar.com.ui">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@drawable/calendar"
            android:supportsRtl="true"
            android:theme="@style/Theme.CalendarAppExample.NoActionBar">
        <activity
                android:name="calendar.com.ui.Stat"
                android:exported="true"/>
        <activity android:name="calendar.com.ui.EventEditActivity"/>
        <activity android:name="calendar.com.ui.WeekViewActivity"/>
        <activity android:name="calendar.com.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>